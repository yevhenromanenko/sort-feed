{
  "name": "linkedin-feed-sorter",
  "version": "0.0.3",
  "description": "Chrome extension to analyze and sort LinkedIn feed posts",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack --mode development --watch",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,json}\" \"*.{json,js}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,json}\" \"*.{json,js}\"",
    "type-check": "tsc --noEmit",
    "package": "npm run build && cd dist && zip -r ../linkedin-feed-sorter.zip . && cd .."
  },
  "keywords": [
    "linkedin",
    "chrome-extension",
    "analytics"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/chrome": "^0.0.268",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.1.8",
    "html-webpack-plugin": "^5.6.0",
    "prettier": "^3.8.1",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.6.2",
    "webpack": "^5.96.1",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "xlsx": "^0.18.5"
  }
}
